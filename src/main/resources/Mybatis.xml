<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC" />
            <dataSource type="com.inno.utils.dbcputils.dbcpDatasourceFactory">
                <property name="driver" value="com.mysql.jdbc.Driver" />
                <property name="url" value="jdbc:mysql://etl2.innotree.org:3308/spider" />
                <property name="username" value="spider" />
                <property name="password" value="spider" />
            </dataSource>
        </environment>
    </environments>

    <mappers>

        <mapper resource="mapping/TestMapper.xml"/>
        <mapper resource="mapping/ReadMapper.xml"/>
        <mapper resource="mapping/WriteMapper.xml"/>
        <mapper resource="mapping/OnlyidMapper.xml"/>
        <mapper resource="mapping/JigouMapper.xml"/>

    </mappers>
</configuration>
